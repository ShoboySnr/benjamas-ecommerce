{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~templates/*": ["components/templates/*"],
      "~atoms/*": ["components/atoms/*"],
      "~molecules/*": ["components/molecules/*"],
      "~organisms/*": ["components/organisms/*"],
      "~helpers/*": ["helpers/*"],
      "~hooks/*": ["hooks/*"],
      "~svgs": ["svgs/*"],
      "~utils/*": ["utils/*"],
      "~types/*": ["types/*"],
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "utils/fauna.js"  ],
  "exclude": [
    "node_modules"
  ]
}
